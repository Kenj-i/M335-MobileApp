<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="M335MobileApp.View.Edit"
             Title="Edit">
    
    <StackLayout Margin="20">
        <Frame Style="{StaticResource F}">
            <StackLayout>
                <Entry x:Name="G" Placeholder="Name of Game"/>
                <Entry x:Name="C" Placeholder="Name of Creator"/>
                <DatePicker Format="dd/MM/yyyy" x:Name="D"/>
                <Button Clicked="EditGame" Text="Edit" x:Name="E" Margin="0, 20"/>
            </StackLayout>
        </Frame>
    </StackLayout>
</ContentPage>